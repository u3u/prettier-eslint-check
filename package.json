{
  "name": "prettier-eslint-check",
  "description": "Check that all files match prettier-eslint code style.",
  "version": "1.0.1",
  "author": "u3u <qwq@qwq.cat>",
  "license": "MIT",
  "homepage": "https://github.com/u3u/prettier-eslint-check#readme",
  "repository": {
    "url": "git+https://github.com/u3u/prettier-eslint-check.git",
    "type": "git"
  },
  "bin": "cli.js",
  "files": [
    "index.js",
    "cli.js"
  ],
  "keywords": [
    "prettier",
    "prettier-eslint",
    "prettier-check",
    "ci",
    "check",
    "lint",
    "style",
    "formatter",
    "code-style"
  ],
  "engines": {
    "node": ">=4"
  },
  "scripts": {
    "test": "ava",
    "lint": "eslint \"{cli,index,test}.js\"",
    "format": "prettier-eslint \"{cli,index,test}.js,*.md\" --write",
    "contributors": "all-contributors"
  },
  "dependencies": {
    "execa": "^0.10.0",
    "prettier": "^1.12.1",
    "prettier-eslint-cli": "^4.7.1"
  },
  "devDependencies": {
    "all-contributors-cli": "^5.0.0",
    "ava": "^0.25.0",
    "eslint": "^5.0.0",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-airbnb-base": "^13.0.0",
    "eslint-plugin-import": "^2.12.0",
    "lint-staged": "^7.1.2",
    "yorkie": "^2.0.0"
  },
  "gitHooks": {
    "pre-commit": "lint-staged"
  },
  "lint-staged": {
    "{cli,index,test}.js": [
      "eslint --fix",
      "git add"
    ],
    "{cli,index,test}.js,*.md": [
      "prettier-eslint --write",
      "git add"
    ]
  }
}
